translate languagetwo kill_pandi_2ced4ec6:

    # "4."
    "4."

# game/script_fish.rpy:536
translate languagetwo kill_pandi_630a4f35_1:

    # "8."
    "8."

# game/script_fish.rpy:537
translate languagetwo kill_pandi_3bdd1c68:

    # "5."
    "5."

